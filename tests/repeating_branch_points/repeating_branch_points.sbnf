SCOPE = `test`

prototype : ( ~'(#+).*\n?'{comment} )* ;

main : ( value ('='{keyword.operator} value)* )* ;

IDENTIFIER = '\b[a-zA-Z_][a-zA-Z_0-9]*\b'

value
: ( number
  | IDENTIFIER{variable.function} `!`{keyword.operator}
  | IDENTIFIER{variable.other}
  )
  `.`{punctuation}*
;

number : '[0-9]+'{constant.numeric} ;
